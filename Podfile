# platform :ios, '9.0'

target 'Amuseum' do
    use_frameworks!
    
    # Helper for Swift KeyValue Observing without Objective-c NSObject
    pod 'SwiftKVC'
    # Firebase helper
    pod 'CodableFirebase'
    # Firebase
    pod 'Firebase/Core'
    pod 'Firebase/Auth'
    pod 'Firebase/Firestore'
    
    target 'AmuseumTests' do
        inherit! :search_paths
        # Pods for testing
    end
    
    target 'AmuseumUITests' do
        inherit! :search_paths
        # Pods for testing
    end
    
end
